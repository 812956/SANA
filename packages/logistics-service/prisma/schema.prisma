generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model FactoryStats {
  id             String @id @default(uuid())
  toysProduced   Int    @default(0)
  coalStockpiled Int    @default(0)
  updatedAt      DateTime @updatedAt
}
